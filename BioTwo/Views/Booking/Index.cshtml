@model List<BioTwo.Models.Booking>

<h2>Bookings</h2>

<table class="table">
   
    
    <thead>
        <tr>
            <th>Booking ID</th>
            <th>Booking Number</th>
            <th>Customer Name</th>
            <th>Customer Email</th>
            <th>Showing Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var booking in Model)
        {
            <tr>
                <td>@booking.BookingID</td>
                <td>@booking.BookingNr</td>
                <td>@booking.CustomerName</td>
                <td>@booking.CustomerEmail</td>
                <td>@booking.Showing.ShowingTime</td>
            </tr>
        }
    </tbody>
</table>
